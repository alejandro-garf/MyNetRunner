# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5432/mynetrunner
DATABASE_USER=postgres
DATABASE_PASSWORD=your_database_password_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# JWT Configuration
# Generate a secure random string (min 64 characters) for production
# Example: openssl rand -base64 64
JWT_SECRET=your_very_long_and_secure_jwt_secret_key_here_minimum_64_characters
JWT_EXPIRATION=86400000

# CORS Configuration
# Frontend URL for CORS (comma-separated for multiple origins)
FRONTEND_URL=http://localhost:5173

# Server Configuration
SERVER_PORT=8080

# Spring Profile (development, production, test)
SPRING_PROFILES_ACTIVE=development
